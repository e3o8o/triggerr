{
  "name": "@triggerr/api-sdk",
  "version": "0.1.0",
  "private": true,
  "description": "Framework-agnostic TypeScript SDK for triggerr parametric insurance platform API",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": "./dist/src/index.js",
    "./client": "./dist/src/client/index.js",
    "./services": "./dist/src/services/index.js",
    "./auth": "./dist/src/auth/index.js",
    "./types": "./dist/src/types/index.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist .turbo",
